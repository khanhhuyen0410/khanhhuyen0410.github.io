<svg width="400" height="240" viewBox="0 0 400 240" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="240" fill="url(#careerGradient)"/>
  
  <!-- Career Growth Chart -->
  <g transform="translate(150, 80)">
    <!-- Chart Bars -->
    <rect x="0" y="60" width="15" height="20" fill="rgba(255,255,255,0.7)" rx="2"/>
    <rect x="20" y="50" width="15" height="30" fill="rgba(255,255,255,0.7)" rx="2"/>
    <rect x="40" y="35" width="15" height="45" fill="rgba(255,255,255,0.7)" rx="2"/>
    <rect x="60" y="20" width="15" height="60" fill="rgba(255,255,255,0.7)" rx="2"/>
    <rect x="80" y="10" width="15" height="70" fill="rgba(255,255,255,0.7)" rx="2"/>
    
    <!-- Arrow -->
    <path d="M5 5l85 0M85 0l5 5-5 5" stroke="rgba(255,255,255,0.8)" stroke-width="2" fill="none"/>
  </g>
  
  <!-- Career Elements -->
  <g opacity="0.8">
    <!-- Skills Icons -->
    <circle cx="80" cy="60" r="12" fill="rgba(255,255,255,0.2)"/>
    <path d="M74 60h12M80 54v12" stroke="rgba(255,255,255,0.7)" stroke-width="2"/>
    
    <circle cx="320" cy="60" r="12" fill="rgba(255,255,255,0.2)"/>
    <rect x="316" y="56" width="8" height="8" fill="rgba(255,255,255,0.7)" rx="1"/>
    
    <circle cx="80" cy="180" r="12" fill="rgba(255,255,255,0.2)"/>
    <path d="M76 180l2-2 2 2-2 2z" fill="rgba(255,255,255,0.7)"/>
    
    <circle cx="320" cy="180" r="12" fill="rgba(255,255,255,0.2)"/>
    <path d="M314 180l6-6 6 6-6 6z" fill="rgba(255,255,255,0.7)"/>
    
    <!-- Labels -->
    <text x="80" y="100" text-anchor="middle" fill="rgba(255,255,255,0.6)" font-size="10" font-family="Arial">Learn</text>
    <text x="320" y="100" text-anchor="middle" fill="rgba(255,255,255,0.6)" font-size="10" font-family="Arial">Build</text>
    <text x="80" y="200" text-anchor="middle" fill="rgba(255,255,255,0.6)" font-size="10" font-family="Arial">Network</text>
    <text x="320" y="200" text-anchor="middle" fill="rgba(255,255,255,0.6)" font-size="10" font-family="Arial">Grow</text>
  </g>
  
  <!-- Text -->
  <text x="200" y="220" text-anchor="middle" fill="rgba(255,255,255,0.9)" font-size="16" font-weight="bold" font-family="Arial">Career Tips</text>
  
  <!-- Gradient Definition -->
  <defs>
    <linearGradient id="careerGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667EEA;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764BA2;stop-opacity:1" />
    </linearGradient>
  </defs>
</svg>
